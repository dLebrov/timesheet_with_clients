{
  "name": "timesheet_with_clients",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "check-typescript": "tsc --noEmit --skipLibCheck",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src/ --max-warnings 0",
    "eslint-fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "stylelint:fix": "stylelint \"src/**/*{.css,.scss}\" --fix",
    "prepare": "husky install",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pre-commit": "yarn check-typescript && yarn eslint-fix --max-warnings=10 && yarn stylelint:fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "antd": "^5.19.2",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "styled-components": "^6.1.12",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "react-router-dom": "^6.25.1",
    "redux-logger": "^3.0.6",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
